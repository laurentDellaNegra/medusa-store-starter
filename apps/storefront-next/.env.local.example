# Your Medusa backend URL
MEDUSA_BACKEND_URL=http://localhost:9000

# Your publishable key (get it from Medusa admin: Settings > API Key Management > Publishable Keys)
NEXT_PUBLIC_MEDUSA_PUBLISHABLE_KEY=

# Your store URL
NEXT_PUBLIC_BASE_URL=http://localhost:8000

# Default region (ISO-2 lowercase)
NEXT_PUBLIC_DEFAULT_REGION=us

# Stripe public key (optional, for Stripe payments)
NEXT_PUBLIC_STRIPE_KEY=

# Medusa payments keys (optional)
NEXT_PUBLIC_MEDUSA_PAYMENTS_PUBLISHABLE_KEY=
NEXT_PUBLIC_MEDUSA_PAYMENTS_ACCOUNT_ID=

# Next.js revalidation secret
REVALIDATE_SECRET=supersecret

# For Cloud users: Medusa Cloud S3 (optional)
MEDUSA_CLOUD_S3_HOSTNAME=
MEDUSA_CLOUD_S3_PATHNAME=
